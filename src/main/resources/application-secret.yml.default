naver:
  client:
    id: [네이버 ID 키]
    secret: [네이버 secret 키]
  url:
    search:
      local: https://openapi.naver.com/v1/search/local.json

google:
  api:
    key: [google 소셜 로그인 api 키]

kakao:
  map:
    appkey: [카카오 소셜 로그인 api 키]

openai:
  api-key: [ai open api 키]

spring:
  security:
    oauth2:
      client:
        registration:
          kakao:
            clientId: [카카오 클라이언트 id 키]
          google:
            client-secret: [구글 클라이언트 시크릿 키]
          naver:
            [네이버 클라이언트 키]

  mail:
    password: [메일 api 키]

custom:
  security:
    oauth2:
      client:
        registration:
          kakao:
            devUser:
              oauthId: oauthId
              nickname: nickname